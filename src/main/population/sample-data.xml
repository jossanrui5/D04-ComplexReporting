                <property name="name" value="Luisa" />
                <property name="surname" value="Camacho" />
                <property name="email" value="luisa.camacho@acme.com" />
            </bean>
        </property>
        <property name="roles">
            <list>
                <ref bean="provider01" />
                <ref bean="authenticatedProvider" />
            </list>
        </property>
    </bean>

    <bean name="authenticatedProvider" class="acme.framework.entities.Authenticated">
        <property name="userAccount" ref="providerUserAccount" />
    </bean>
    
    
    <bean name="auditorUserAccount" class="acme.framework.entities.UserAccount">
        <property name="username" value="auditor" />
        <property name="password" value="auditor" />
